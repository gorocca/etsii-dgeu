{
  "name": "etsii",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/react-bootstrap": "^0.32.15",
    "i": "^0.3.6",
    "npm": "^6.5.0",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.4",
    "react-calendar": "^2.17.5",
    "react-dom": "^16.7.0",
    "react-scripts-ts": "^4.0.8"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts-ts start",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass --include-path ./node_modules src/assets/scss -o ./src/assets/css --recursive",
    "watch-css": "npm run build-css && node-sass --include-path ./node_modules src/assets/scss -o ./src/assets/css --recursive --watch"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "bootstrap-scss": "^4.1.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
